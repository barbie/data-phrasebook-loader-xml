<?xml version="1.0"?>
 <!DOCTYPE phrasebook [
	       <!ELEMENT phrasebook (dictionary)*>              
	       <!ELEMENT dictionary (phrase)*>
               <!ATTLIST dictionary name CDATA #REQUIRED>
               <!ELEMENT phrase (#PCDATA)>
               <!ATTLIST phrase name CDATA #REQUIRED>
 ]>
 
 <phrasebook>
 <dictionary name="BASE">
   <phrase name="foo">
     Welcome to [% my %] world.
     It is a nice [%place %].
   </phrase>
   <phrase name="bar">
     Welcome to :my world. 
     It is a nice :place.
   </phrase>
    <phrase name="baz">
  1
 2 
 3
   </phrase>
</dictionary>
 </phrasebook>
